<h1>投稿一覧</h1>
<%= link_to "投稿", new_list_path %>
<% @lists.each do |list| %>
  <p>タイトル</p>
  <span><%= link_to list.title, list_path(list.id) %></span>

  <p>画像</p>
  <% if list.image.attached? %>
    <%= image_tag list.image, size: "100x100" %>
  <% else %>
    <%= image_tag 'no_image', size: "100x100" %>
  <% end %>

<% end %>